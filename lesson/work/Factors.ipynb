    "#Find factors\n",
    "try:\n",
    "    value=int(input('input a numerical value:'))\n",
    "    print(f'{value} factors are',end=':')\n",
    "    for i in range(1,value+1):\n",
    "        if value % i ==0:\n",
    "            print(i,end=' ')\n",
    "except:\n",
    "    print('input format error.')"
   ]
